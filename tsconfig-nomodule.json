{
	"compilerOptions": {
		"alwaysStrict": true,
		"declaration": false,
		"downlevelIteration": true,
		"esModuleInterop": false,
		"experimentalDecorators": false,
		"forceConsistentCasingInFileNames": true,
		"newLine": "LF",
		"module": "none",
		"moduleResolution": "node",
		"pretty": true,
		"resolveJsonModule": false,
		"types": [
			"node",
			"browserify",
			"del",
			"gulp-pug",
			"sass-embedded"
		],
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"sourceMap": true,
		"strictNullChecks": true,
		"skipLibCheck": true,
		"target": "ESNext",
		"rootDir": "."
	},
	"files": [
		"./browserViews/js/_fix_exports.ts",
		"./browserViews/js/_import-map-loader.ts"
	],
	"include": [
	],
	"exclude": [
		"**/*.ts"
	]
}
