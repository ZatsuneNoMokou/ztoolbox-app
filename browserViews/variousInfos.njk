{%- if versions -%}
	<p>Node.js {{versions.node}}, Chromium {{versions.chrome}}, Electron {{versions.electron}} (current i18next language :&nbsp;<span data-translate-id='language'></span>)</p>
{%- endif -%}

{%- if versionState -%}
	<p>Version actuelle bas√©e sur la branche {{versionState.branch}}, commit du {{versionState.commitDate.toLocaleString()}}.</p>
{%- endif -%}

{%- if processArgv|length > 0 -%}
	<p>Arguments : {{processArgv|join(', ')}}</p>
{%- endif -%}

{%- if networkIps -%}
	<p>Adresses IPs&nbsp;:</p><ul>
	{%- for name, value in networkIps -%}
		{%- if value.ipV4 | first !== "127.0.0.1" -%}
			<li>{{ name }} : <p>{{ value.ipV4 | join(', ') }} / {{ value.ipV6 | join(', ') }}</p></li>
		{%- endif -%}
	{%- endfor -%}</ul>
{%- endif -%}
