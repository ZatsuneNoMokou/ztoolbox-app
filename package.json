    "browserify": "^17.0.0",
    "cron-parser": "^4.9.0",
    "esm": "npm:esm-wallaby@^3.2.31",
    "fastify": "^4.27.0",
    "flems": "^1.5.10",
    "fs-extra": "^11.2.0",
    "i18next": "^23.11.5",
    "ini": "^4.1.3",
    "json5": "^2.2.3",
    "lodash.debounce": "^4.0.8",
    "nunjucks": "^3.2.4",
    "protocol-registry": "^1.4.1",
    "shelljs": "^0.8.5",
    "simple-git": "^3.24.0",
    "socket.io": "^4.7.5",
    "string-strip-html": "^13.4.8",
    "ws": "^8.18.0",
    "yaml": "^2.4.2"
  }
